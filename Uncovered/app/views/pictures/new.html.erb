<h2>Input the required fields to post a new picture! </h2>

<%= form_for @picture_entry, html: { multipart: true } do |f| %>
	<%= f.label :caption %>
	<%= f.text_field :caption %>

	<%= f.label :location %>
	<%= f.text_field :location %>

	<%= f.lavel :image_file_name %>
	<%= f.file_field :image_file_name %>

	<%= f.button %>
<%end%>
